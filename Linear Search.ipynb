    "def linearSearch(arr,key):\n",
    "    size=len(arr)\n",
    "    for i in range(size):\n",
    "        if(key==arr[i]):\n",
    "            return print(\"Element is found at index {x}\".format(x=i))\n",
    "    return print(\"Not Found\")"
