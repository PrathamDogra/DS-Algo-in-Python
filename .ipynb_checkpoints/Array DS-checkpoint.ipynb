   "source": [
    "# Array Reversal\n",
    "def reverseArray(a):\n",
    "    n = len(a)\n",
    "    start =0\n",
    "    end = n-1\n",
    "    while(start<end):\n",
    "        a[start], a[end] = a[end], a[start]\n",
    "        start+=1\n",
    "        end-=1\n",
    "    return a"
   ]
  },
